# Development Environment Configuration
NEXT_PUBLIC_ENV=development

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-dev-access-key
AWS_SECRET_ACCESS_KEY=your-dev-secret-key

# Cognito Configuration
AWS_COGNITO_REGION=us-east-1
AWS_COGNITO_USER_POOL_ID=us-east-1_DevPoolId
AWS_COGNITO_USER_POOL_CLIENT_ID=dev-client-id
AWS_COGNITO_IDENTITY_POOL_ID=us-east-1:dev-identity-pool-id

# S3 Configuration
AWS_S3_BUCKET_NAME=abditrade-dev-cache
AWS_S3_REGION=us-east-1

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=https://dev-opensearch.us-east-1.es.amazonaws.com
OPENSEARCH_INDEX=cards-dev

# DynamoDB Configuration
DYNAMODB_TABLE_PREFIX=abditrade-dev-

# Backend API Configuration
NEXT_PUBLIC_BACKEND_API_URL=https://dev-api.abditrade.com
BACKEND_API_KEY=dev-api-key

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_dev_stripe_key
STRIPE_SECRET_KEY=sk_test_dev_stripe_secret
STRIPE_WEBHOOK_SECRET=whsec_dev_webhook_secret

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=dev-nextauth-secret-change-in-production

# Database Configuration (if needed)
DATABASE_URL=postgresql://dev-user:dev-pass@dev-db.amazonaws.com:5432/abditrade_dev

# External API Configuration
YUGIOH_API_URL=https://db.ygoprodeck.com/api/v7/cardinfo.php
POKEMON_API_URL=https://api.pokemontcg.io/v2/cards
MAGIC_API_URL=https://api.scryfall.com/cards/search

# Monitoring and Logging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

# Cache Configuration
CACHE_TTL_HOURS=1
ENABLE_S3_CACHE=true

# Feature Flags
ENABLE_OPENSEARCH=false
ENABLE_BACKEND_API=false
ENABLE_REAL_PAYMENTS=false